<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd"><configuration>    <!--设置与数据库字段名与bean的驼峰映射-->    <settings>        <setting name="mapUnderscoreToCamelCase" value="true"/>    </settings>    <!--为实体类起别名-->    <typeAliases>        <package name="com.qst.goldenarches.pojo"/>    </typeAliases>    <!--配置pageHelper分页插件-->    <plugins>        <plugin interceptor="com.github.pagehelper.PageInterceptor">            <!--分页参数合理化  -->            <property name="reasonable" value="true"/>        </plugin>    </plugins></configuration>